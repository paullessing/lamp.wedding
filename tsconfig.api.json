  "compilerOptions": {
    "sourceMap": true,
    "target": "es6",
    "lib": [
      "es2015",
      "esnext"
    ],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "typeRoots": [
      "node_modules/@types",
      "types"
    ]
  },
  "exclude": [
    "node_modules",
    "frontend"
  ]
}
